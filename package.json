{
  "name": "documentation",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --disable-host-check --host 0.0.0.0",
    "build": "ng build --prod",
    "build:prerender": "ng run intellij-ron-plugin:prerender:production",
    "build:prerenderer": "tsc -p prerenderer/tsconfig.json",
    "prerender": "npm run build:prerenderer && cd dist && node prerenderer",
    "release": "npm run build && npm run build:prerender && npm run prerender",
    "serve": "cd dist && open http://localhost:8080/intellij-ron-plugin && http-server",
    "gh-pages": "npx ngh --dir=dist/intellij-ron-plugin"
  },
  "private": true,
